<div class="text-align-center">
  <div class="row">
    <div class="col-1">
      <h6 class="verticaltext"><%= (I18n.t "astroboard.signs").upcase %></h6>
    </div>

    <div class="col-10 border">
      <div class="row">
        <div class="col-5 px-2">
          <% @my_signs.each do |sign| %>
            <div>
              <div class="cell">
                <div>
                  <h6>
                    <%= I18n.t "signs.#{sign}" %> <%# voir si l'on peut mettre une classe transparente quand ce n'est pas sign_group.first %>
                  </h6>
                </div>
              </div>
            </div>
          <% end %>
        </div>

        <div class="col-5 px-2 planets-frame">
          <div>
            <% @my_planets.each do |planet| %>
              <div class="cell">
                <h6>
                  <%= "#{planet.first} #{(I18n.t "planets.#{planet.last}.").upcase}" %>
                </h6>
              </div>
            <% end %>
          </div>
        </div>

        <div class="col-2 px-2">
          <div>
            <% @my_houses.each do |house_group| %>
              <% house_group.each do |house| %>
                <div class="cell">
                  <div class="house">
                    <h6>
                      <%= house %>
                    </h6>
                  </div>
                </div>
              <% end %>
            <% end %>
          </div>
        </div>
      </div>
    </div>

    <div class="col-1 d-flex text-center">
      <h6 class="verticaltext"><%= (I18n.t "astroboard.houses").upcase %></h6>
    </div>
  </div>
</div>



